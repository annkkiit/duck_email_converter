{
  "manifest_version": 3,
  "name": "Duck Email Converter",
  "version": "1.0.0",
  "homepage_url": "https://github.com/z-aki/duck_email_converter",
  "author": "z-aki",
  "description": "Convert your forwarding email and your DuckDuckGo Email Protection username to an email ready to be written to. Supports [at][dot].",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "icon128.png",
      "300": "icon300.png"
    }
  },
  "permissions": [
    "clipboardWrite",
    "storage"
  ],
  "icons": {
    "128": "icon128.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "duck_email_converter@z-aki",
      "strict_min_version": "109.0"
    }
  }
}